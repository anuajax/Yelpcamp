<% include ./partials/header %>
<div class="container login">
    <form action="/change" method="POST">
    <div class="form-group">
        <input type="password" class="form-control" name="oldPassword" id="password" placeholder="old password">
    </div>
    <div class="form-group">
        <input type="password" class="form-control" name="newPassword" id="new password" placeholder="new password"></div>
        <div class="form-group">
        <input type="password" class="form-control" name="confirmnewPassword" id="confirm new password" placeholder="confirm new password"></div>
        <div class="form-group">
        <input type="submit" class="btn btn-dark form-control"  value="change password"></div>
    </div>
    </form>
</div>
<% include ./partials/footer %>
